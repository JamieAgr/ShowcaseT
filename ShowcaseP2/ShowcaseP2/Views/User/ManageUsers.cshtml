@model IEnumerable<ApplicationUser>

<h2>User Management</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>
                <td>
                    <a href="@Url.Action("Edit", "User", new { id = user.Id })">Edit</a> |
                    <a href="@Url.Action("Delete", "User", new { id = user.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
